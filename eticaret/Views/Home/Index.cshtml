@model AnasayfaModeli



@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    
    @foreach (var item in Model.Kullanicilar)
    {
    <h1 class="display-4">@item.adSoyad</h1>


    }
       <hr />

       @foreach (var item in Model.Arabalar)
    {
        <h1 class="display-2">@item.Marka - @item.Model - @item.Yil</h1>
    }
    @Model.Sehir

    
    @foreach (var item in Model.Bilgisayarlar)
    {
        <h1 class="display-3">@item.marka + @item.islemci + @item.ekranBoyutu </h1>
    }
    @for (int i = 0; i < Model.Bilgisayarlar.Count(); i++)
    {
        <h1 class="display-3">@Model.Bilgisayarlar[i].marka + @Model.Bilgisayarlar[i].islemci + @Model.Bilgisayarlar[i].ekranBoyutu </h1>
    }
</div>
